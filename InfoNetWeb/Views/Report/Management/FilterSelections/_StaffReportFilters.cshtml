@using Infonet.Reporting.Enumerations
@model Infonet.Reporting.ViewModels.ManagementReportViewModel

<div class="panel panel-primary icjia-error-group reportProperty @SubReportSelection.MngRptStaffServiceStaffReport.GetShortName()" style="display:none">
	<div class="panel-heading">
		<h2 class="panel-title">
			<span class="icjia-panel-title-text">Filters</span>
		</h2>
	</div>
	<div class="panel-body">
		<div class="form-group">
			@Html.LabelFor(m => m.StaffReportStaffSelections)
			@Html.DropDownListFor(model => model.StaffReportStaffSelections, new MultiSelectList(Model.StaffDefault, "SVID", "StaffName", Model.StaffReportStaffSelections), new { @class = "form-control chosen staffSelection", multiple = "multiple" })
		</div>
	</div>
</div>
